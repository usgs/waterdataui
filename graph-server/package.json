{
  "name": "waterdataui-graph-server",
  "version": "0.30.0dev",
  "description": "Water Data For The Nation graph server renderer",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc jasmine",
    "start": "DEBUG=express:* node src/index.js",
    "watch": "run-p watch:*",
    "watch:server": "DEBUG=express:* STATIC_ROOT=http://localhost:9000 nodemon src/index.js",
    "watch:swagger": "nodemon -w src/index.js -w src/swagger-def.js -x \"npm run build:swagger\"",
    "build:swagger": "swagger-jsdoc -d src/swagger-def.js -o src/swagger.json"
  },
  "engines": {
    "node": "10.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usgs/waterdataui.git"
  },
  "author": "USGS",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/usgs/waterdataui/issues"
  },
  "homepage": "https://github.com/usgs/waterdataui#readme",
  "dependencies": {
    "axios": "0.19.2",
    "express": "4.17.1",
    "express-cache-headers": "0.1.4",
    "express-validator": "6.4.0",
    "generic-pool": "3.7.1",
    "puppeteer": "2.1.1",
    "swagger-ui-express": "4.1.4"
  },
  "devDependencies": {
    "decache": "^4.5.1",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^4.0.0"
  }
}
